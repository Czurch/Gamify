{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "Gamify",
    "slug": "Gamify",
    "version": "0.2.1",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Gamify to use your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "http://127.0.0.1:4000/graphql"
      }
    }
  }
}
